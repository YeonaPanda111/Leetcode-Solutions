class Solution:
    def rearrangeArray(self, nums: List[int]) -> List[int]:
        posi=[]
        negi=[]
        ans=[]
        for i in nums:
            if i>=0:
                posi.append(i)
            else:
                negi.append(i)
        for i,j in zip(posi,negi):
            ans.append(i)
            ans.append(j)
        return ans

# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def reverseList(self, head: Optional[ListNode]) -> Optional[ListNode]:
        if head!=None:
            p=head
            if p.next==None:
                return p

            c=p.next
            p.next=None

            if c.next==None:
                c.next=p
                
                return c

            n=c.next
            
            while(n):
                c.next=p
                p=c
                c=n
                n=n.next

            c.next=p
            return c
            
        return None

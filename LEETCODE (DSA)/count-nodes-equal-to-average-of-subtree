# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, val=0, left=None, right=None):
#         self.val = val
#         self.left = left
#         self.right = right
class Solution:
    def averageOfSubtree(self, root: TreeNode) -> int:
        ans=0
        
        if not root:
            return 0

        def f(root):
            if not root:
                return None
            
            self.sumi+=root.val
            self.n+=1
            f(root.left)
            f(root.right)



        self.sumi=0
        self.n=0
        f(root)
        if self.sumi//self.n==root.val:
            ans+=1

        ans+=self.averageOfSubtree(root.left)
        ans+=self.averageOfSubtree(root.right)

        return ans

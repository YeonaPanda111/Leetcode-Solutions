# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def modifiedList(self, nums: List[int], head: Optional[ListNode]) -> Optional[ListNode]:
        s = set(nums)
        p1=head
        p2=None
        p3=None

        while(p1):
            if p1.val not in s:
                if p2 is None:
                    p2=p1
                    p3=p2
                
                else:
                    p3.next=p1
                    p3=p1
                
            p1=p1.next

        if p3:
            p3.next=None


        return p2

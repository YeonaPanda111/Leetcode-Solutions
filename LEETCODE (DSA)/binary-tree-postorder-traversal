# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, val=0, left=None, right=None):
#         self.val = val
#         self.left = left
#         self.right = right
class Solution:
    def postorderTraversal(self, root: Optional[TreeNode]) -> List[int]:
        l=[]
        
        def f(root):
            if not root:
                return 

            left=f(root.left)
            right=f(root.right)
            l.append(root.val)

        f(root)
            

        return l

class Solution:
    def findPermutationDifference(self, s: str, t: str) -> int:
        ans=0
        el=[0]*26
        for idx, e in enumerate(s):
            el[ord(e)-97]+=idx

        for idx, e in enumerate(t):
            el[ord(e)-97]-=idx

        for i in el:
            ans+=abs(i)
        return ans
